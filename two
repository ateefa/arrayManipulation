
def checkMagazine(magazine, note):
	
	for i in note:
		if i not in magazine:
			return "No"
			break
		magazine.remove(i)	
	return("Yes")			



mn = input().split()

m = int(mn[0])
n = int(mn[1])
magazine = input().rstrip().split()

note = input().rstrip().split()
print(checkMagazine(magazine, note))
